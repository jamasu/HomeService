@model IEnumerable<HomeService.Controllers.ClimateData>

<table>

	<thead>
	<th>Lokasjon</th>
	<th>Temperatur</th>
	<th>Fuktighet</th>
	<th>Tidskontroll</th>

	@foreach (var data in ViewData["ClimateData"] as IEnumerable<HomeService.Controllers.ClimateData>)
	{

		<tr>
			<td>
				@data.Location
			</td>

			
			<td>
				@{var temp = data.Temperature.Split('#');
				String.Concat(temp,Html.Raw(((char)176).ToString()))
			}
			</td>


			<td>
				@data.Humidity
			</td>


			<td>
				@data.Timestamp
			</td>
		</tr>

	}

	</thead>
</table>
