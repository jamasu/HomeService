using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace HomeService.Business
{
    public static class Helper
    {
        public static string ToLower(string s)
        {
            char[] temp = new char[s.Length];
            Dictionary<char, char> toLower = new Dictionary<char, char>();
            toLower.Add('A', 'a');
            toLower.Add('B', 'b');
            toLower.Add('C', 'c');
            toLower.Add('D', 'd');
            toLower.Add('E', 'e');
            toLower.Add('F', 'f');
            toLower.Add('G', 'g');
            toLower.Add('H', 'h');
            toLower.Add('I', 'i');
            toLower.Add('J', 'j');
            toLower.Add('K', 'k');
            toLower.Add('L', 'l');
            toLower.Add('M', 'm');
            toLower.Add('N', 'n');
            toLower.Add('O', 'o');
            toLower.Add('P', 'p');
            toLower.Add('Q', 'q');
            toLower.Add('R', 'r');
            toLower.Add('S', 's');
            toLower.Add('T', 't');
            toLower.Add('U', 'u');
            toLower.Add('V', 'v');
            toLower.Add('W', 'w');
            toLower.Add('X', 'x');
            toLower.Add('Y', 'y');
            toLower.Add('Z', 'z');
            toLower.Add('Æ', 'æ');
            toLower.Add('Ø', 'ø');
            toLower.Add('Å', 'å');

            for (int i =0; i < s.Length; i ++)
            {
              
                temp[i] = toLower[s.ElementAt(i)];

            }

            return temp.ToString();
        }
    }
}
